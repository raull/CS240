package client.batch.state;

import java.util.ArrayList;

public class BatchState {
	
	private static ArrayList<BatchStateListener> listeners = new ArrayList<BatchStateListener>();
	private String[][] values;
	private static Cell selectedCell;
	
	private static BatchState instance = null;
	
	public static BatchState singleton() {
		if (instance == null) {
			return new BatchState();
		} else {
			return instance;
		}
	}
	
	public static void setValues(String[][] values) {
		BatchState.singleton().values = values;
	}
	
}
